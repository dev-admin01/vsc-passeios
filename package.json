{
  "name": "vsc-passeios",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "trap 'exit $?' INT TERM; npm run services:up && npm run services:wait:database && npm run migrations:up && next dev",
    "test": "npm run services:up && concurrently -n next,jest --hide next -k -s command-jest \"next dev\" \"jest --runInBand --verbose\"",
    "build": "next build",
    "start": "next start",
    "services:up": "docker compose -f src/infra/compose.yaml up -d",
    "services:stop": "docker compose -f src/infra/compose.yaml stop",
    "services:down": "docker compose -f src/infra/compose.yaml down",
    "services:wait:database": "node src/infra/scripts/wait-for-postgres.js",
    "migrations:create": "node-pg-migrate -m src/infra/migrations create",
    "migrations:up": "node-pg-migrate -m src/infra/migrations --envPath .env.development up",
    "test:watch": "jest --watchAll --runInBand --verbose",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:eslint:check": "next lint"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@types/jest": "29.5.14",
    "async-retry": "^1.3.3",
    "axios": "^1.8.2",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookies-next": "^5.1.0",
    "dotenv": "16.4.7",
    "dotenv-expand": "^11.0.6",
    "jose": "^6.0.8",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.477.0",
    "next": "15.2.1",
    "next-connect": "1.0.0",
    "node-pg-migrate": "7.9.1",
    "pg": "8.13.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "29.2.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3",
    "@tailwindcss/postcss": "4",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/async-retry": "^1.4.9",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "20",
    "@types/node-pg-migrate": "2.3.1",
    "@types/pg": "8.11.11",
    "@types/react": "19",
    "@types/react-dom": "19",
    "concurrently": "^8.2.2",
    "eslint": "9",
    "eslint-config-next": "15.2.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "^3.5.3",
    "tailwindcss": "4",
    "ts-node": "10.9.2",
    "typescript": "5"
  }
}
