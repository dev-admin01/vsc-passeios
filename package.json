{
  "name": "vsc-passeios",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run services:up && npm run services:wait:database && npm run migrations:up && next dev",
    "services:up": "docker compose -f infra/compose.yml up -d",
    "services:down": "docker compose -f infra/compose.yml down",
    "services:stop": "docker compose stop",
    "test": "npm run services:up && concurrently -n next,jest --hide next -k -s command-jest \"TESTING_MODE=true next dev\" \"TESTING_MODE=true jest --runInBand --verbose\"",
    "vercel-build": "npm run prisma:generate && prisma migrate deploy && npm run prisma:seed && next build",
    "build": "npm run prisma:generate && prisma migrate deploy && npm run prisma:seed && next build",
    "start": "next start",
    "test:watch": "TESTING_MODE=true jest --watchAll --runInBand --verbose",
    "services:wait:database": "node infra/scripts/wait-for-postgres.js",
    "migrations:up": "npx prisma migrate dev",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:eslint:check": "next lint",
    "prisma:generate": "npx prisma generate",
    "prisma:seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.10.1",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-email/components": "^0.0.34",
    "@react-pdf/renderer": "^4.3.0",
    "@types/jest": "29.5.14",
    "@types/js-cookie": "^3.0.6",
    "async-retry": "^1.3.3",
    "axios": "^1.8.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookies-next": "^5.1.0",
    "dotenv": "16.4.7",
    "dotenv-expand": "^11.0.6",
    "jose": "^6.0.11",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.477.0",
    "next": "15.2.1",
    "next-connect": "1.0.0",
    "node-pg-migrate": "7.9.1",
    "pg": "8.13.3",
    "prisma": "^6.10.1",
    "react": "19.0.0",
    "react-currency-input-field": "^3.10.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.55.0",
    "react-pdf": "^9.2.1",
    "recharts": "^2.15.1",
    "resend": "^4.2.0",
    "sonner": "^2.0.3",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "29.2.6",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3",
    "@faker-js/faker": "^9.8.0",
    "@tailwindcss/postcss": "4",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/async-retry": "^1.4.9",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "20",
    "@types/node-pg-migrate": "2.3.1",
    "@types/pg": "8.11.11",
    "@types/react": "19",
    "@types/react-dom": "19",
    "concurrently": "^8.2.2",
    "eslint": "9",
    "eslint-config-next": "15.2.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "^3.5.3",
    "tailwindcss": "4",
    "ts-node": "10.9.2",
    "typescript": "5"
  }
}
